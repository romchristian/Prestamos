	<queryString>
		<![CDATA[select p.monto as monto,p.vencimiento as vencimiento,b.nombre as banco,p.descripcion as descripcion
from pago p inner join banco b
on p.banco_id = b.id
join transaccion t
on t.id = p.transaccion_id
where
t.sesiontpv_id = $P{idSesionTPV}
order by b.nombre]]>
	</queryString>
